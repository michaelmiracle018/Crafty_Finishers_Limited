{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "turbo format lint typecheck coverage build knip",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "knip": "knip",
    "lint": "eslint --color --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "lint --fix",
    "prepare": "is-ci || lefthook install",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc"
  },
  "prettier": "@jimmy.codes/prettier-config",
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.0"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.5",
    "@tanstack/react-router": "1.133.36",
    "framer-motion": "^12.23.24",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-odometerjs": "^3.1.3",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@iconify-json/logos": "1.2.10",
    "@iconify-json/lucide": "1.2.71",
    "@jimmy.codes/eslint-config": "6.15.0",
    "@jimmy.codes/prettier-config": "2.1.1",
    "@playwright/test": "1.56.1",
    "@tanstack/react-query-devtools": "5.90.2",
    "@tanstack/react-router-devtools": "1.133.36",
    "@tanstack/router-vite-plugin": "1.133.36",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/bun": "1.3.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "@vitest/coverage-v8": "4.0.6",
    "@vitest/ui": "4.0.6",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "eslint": "^9.38.0",
    "gitzy": "6.5.2",
    "happy-dom": "20.0.10",
    "is-ci": "4.1.0",
    "jiti": "2.6.1",
    "knip": "5.66.4",
    "lefthook": "2.0.2",
    "prettier": "^3.6.2",
    "turbo": "2.6.0",
    "typescript": "5.9.3",
    "vite": "npm:rolldown-vite@7.1.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.6"
  },
  "packageManager": "yarn@1.22.22",
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "oxc-resolver",
    "unrs-resolver"
  ]
